	/* eMMC 4.5
	dwmmc0: dwmmc0@FC183000 {
		compatible = "hisilicon,kirin980-dw-mshc";
		reg = <0x0 0xfc183000 0x0 0x1000>;
		interrupts = <0 140 4>;
		board-mmc-bus-clk = <20000000>;
		clocks = <&clk_gate_sdio>, <&hclk_gate_sdio>;
		clock-names = "ciu", "biu";
		interrupt-parent = <&gic>;
		vmmc-supply=<&ldo15>;
		num-slots = <1>;
		fifo-depth = <0x100>;
